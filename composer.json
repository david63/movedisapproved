{
	"name": "david63/movedisapproved",
	"type": "phpbb-extension",
	"description": "Moves disapproved posts to another forum.",
	"version": "3.3.0-RC1",
	"time": "2020-12-21",
	"keywords":
	[
		"phpbb",
		"extension",
		"posts",
		"disapprove"
	],
	"license": "GPL-2.0-only",
	"authors": [{
		"name": "David Wood",
		"role": "Developer"
	}],
	"require": {
        "php": ">=7.1.3"
    },
	"extra": {
		"display-name": "Move disapproved posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      },
	  "version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "move_disapproved_version_file.json",
			"ssl": false
    	}
	}
}
